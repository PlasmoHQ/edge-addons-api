{
  "name": "@plasmohq/edge-addons-api",
  "version": "1.0.0",
  "description": "Microsoft Edge Addons API for Node.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
