    "edge",
    "webstore",
    "upload",
    "add-ons"
  ],
  "dependencies": {
    "got": "12.1.0"
  },
  "devDependencies": {
    "@jest/globals": "28.1.0",
    "@plasmohq/rps": "1.3.3",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "17.0.34",
    "cross-env": "7.0.3",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.2",
    "tsup": "5.12.8",
    "typescript": "4.6.4"
  }
